package com.ssafy.ws.step3;

/**
 * 모래시계 모양의 숫자 출력하는 클래스
 */
public class DigitTest2 {

	public static void main(String[] args) {
		int[] arr = {5,3,1,3,5};
		// 5 -> 3 -> 1 -> 3 -> 5
		for(int i=1;i<18;i++) {
			
			if(i>=10)System.out.print(i+" ");
			else System.out.print(i+"  ");
			
			
		}
	}
}
